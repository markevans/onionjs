define([
  'onion/controller',
  '<%= module_dirname %>/<%= view_basename %>'
], function (
  Controller,
  <%= view_name %>
) {

  return Controller.sub('<%= controller_name %>')

    .view(<%= view_name %>)

    .after('init', function () {
      this.view.render({me: '<%= basename %>'})
    })

})
