define [
  'onion/view'
  'onion/mustache_view'
  'onion/vendor/text!<%= module_dirname %>/<%= template_filename %>'
], (
  View
  mustacheView
  template
) ->

  class <%= view_name %> extends View

    @use mustacheView

    render: ->
      @renderMustache(template, @models, me: '<%= view_name %>')

