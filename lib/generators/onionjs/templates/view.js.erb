define([
  'onion/view',
  'onion/mustache_view',
  'onion/vendor/text!<%= module_dirname %>/<%= template_filename %>'
], function (
  View,
  mustacheView,
  template
) {

  return View.sub('<%= view_name %>')

    .use(mustacheView)

    .proto({
      render: function () {
        this.renderMustache(template)
      }
    })

})
